{{#each this}}
<div class="card">
    <div class="head-box">
        <h2 class="head">{{name}}</h2>
    </div>
    <div class="content">
        <div>
            <p class="capit">Capital: <span class="textSpa">{{capital}}</span></p>
            <p class="popul">Population: <span class="textSpa">{{population}}</span></p>
            <p class="lang">Languages:
            <ul class="list">
                {{#each languages}} <li>{{name}}</li>{{/each}}

            </ul>
            </p>
        </div>
        <img src="{{flag}}" alt="{{name}} width=" 200" height="200"">

    </div>
</div>
{{/each}}